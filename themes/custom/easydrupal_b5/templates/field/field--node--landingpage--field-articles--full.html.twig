<div class="section articles" id="articles">
  <div class="container-lg">
    <div class="title-wrapper">
      <h2 class="section-title style-fancy fancy-centered">{{ label }}</h2>
    </div>
    <div class="row">
      {% for item in items %}
        <div class="col-sm-6">
          {{ item.content }}
        </div>
      {% endfor %}
    </div>
    <div class="read-more"><a href="/articles">{{ "Look other articles"|t }} ></a></div>
  </div>
</div>
