<div class="section clients" id="clients">
  <div class="container-lg">
    <div class="title-wrapper">
      <h2 class="section-title animated field-label-above">{{ label }}</h2>
      <div class="line"></div>
    </div>

    <div class="row">
      {% for item in items %}
        <div class="item col-sm-6 col-md-2 col-lg-2">
          {{ item.content }}
        </div>
      {% endfor %}
    </div>
  </div>
</div>
